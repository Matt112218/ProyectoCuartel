{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:react/recommended",
    "eslint:recommended",
    "next"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "proyect": "./tsconfig.json"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "off"
  }
}
